[distutils]
  index-servers =
    pypi
    testpypi
    sensecam-discovery

[pypi]
  username = __token__
  password = ${{ secrets.PYPI_Password }}

[testpypi]
  username = __token__
  password = ${{ secrets.TEST_PYPI_Password }}

[PROJECT_NAME]
  repository = https://upload.pypi.org/legacy/
  username = __token__
  password = ${{ secrets.PYPI_Password }}
